<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HuffingtonPost" parallel="none">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows 10"/>
    <parameter name="os_version" value="10.0"/>
    <parameter name="browser_name" value="chrome"/>
    <parameter name="browserVersion" value="63.0"/>
    <parameter name="url" value="https://www.huffingtonpost.com/"/>

    <test name="Test Huffington Post">
        <classes>
            <class name="testcaseshuff.TestHomePage"/>
            <class name="testcaseshuff.TestMenuBar"/>
            <class name="testcaseshuff.TestSubscribeNews"/>
            <class name="testcaseshuff.TestPoliticsCategory"/>
            <class name="testcaseshuff.TestNews"/>
        </classes>
    </test>
</suite>